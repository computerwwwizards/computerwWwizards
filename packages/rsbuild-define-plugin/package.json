{
  "name": "@computerwwwizards/rsbuild-define-plugin",
  "version": "1.0.0",
  "type": "module",
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      }
    },
    "main": "./dist/index.cjs",
    "types": "./dist/index.d.ts"
  },
  "main": "./src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "check": "biome check --write",
    "dev": "rslib build --watch",
    "format": "biome format --write",
    "test:integration": "playwright test"
  },
  "dependencies": {
    "@computerwwwizards/define-core": "workspace:*"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.3",
    "@rslib/core": "^0.12.4",
    "@types/node": "^22.18.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "optionalDependencies": {
    "@playwright/test": "^1.55.0",
    "playwright": "^1.55.0"
  },
  "peerDependencies": {
    "@rsbuild/core": "1.5.1"
  },
  "private": false
}